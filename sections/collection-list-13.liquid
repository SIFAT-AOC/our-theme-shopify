                   .c-l-13-img-container {
                     position: relative;
                   }
  .c-l-13-img-container .c-l-13-imgTi {
	position: absolute;
	top: 85%;
	left: 50%;
	transform: translate(-50%, -50%);
	padding: 9px 10px;
	text-align: center;
	height: auto;
	display: block;
	width: 85%;
	background-color: white;
}

.c-l-13-img-container .c-l-13-imgTi2 {
	position: absolute;
	top: 84%;
	left: 50%;
	transform: translate(-50%, -50%);
	padding: 8px 10px;
	text-align: center;
	/* height: auto; */
	display: block;
	width: 85%;
	background-color: white;
}
                   .c-l-13-p-m {
                     margin-bottom: 10px;
                     padding: 0px 5px;
                   }

                   .c-l-13-img-container {
                     overflow: hidden;
                   }
                   .c-l-13-img-container:hover img {
                     transform: scale(1.1);
                     transition: transform 0.9s ease;
                     z-index: 1;
                   }
                   /* hover effect css code  */
                   .c-l-13-imgeEffect {
                     position: relative;
                     overflow: hidden;
                   }

                   .c-l-13-imgeEffect::before,
                   .c-l-13-imgeEffect::after,
                   .c-l-13-imgeEffect > .top-right::before,
                   .c-l-13-imgeEffect > .top-right::after,
                   .c-l-13-imgeEffect > .bottom-left::before,
                   .c-l-13-imgeEffect > .bottom-left::after {
                     content: "";
                     position: absolute;
                     width: 50%;
                     height: 50%;
                     background-color: transparent;
                     opacity: 0.9;
                     transform-origin: center;
                     transition: all 0.5s ease-out;
                     z-index: 1;
                     opacity: 0;
                   }
                   .c-l-13-imgeEffect > img {
                     display: block;
                     width: 100%;
                     height: auto;
                     transition: all 0.4s ease-out;
                     z-index: 1;
                   }

                   .c-l-13-imgeEffect::before {
                     top: 0;
                     left: 0;
                     background-color: transparent;
                   }

                   .c-l-13-imgeEffect::after {
                     bottom: 0;
                     right: 0;
                     background-color: transparent;
                   }

                   .c-l-13-imgeEffect > .top-right::before {
                     top: 0;
                     right: 0;
                   }

                   .c-l-13-imgeEffect > .top-right::after {
                     bottom: 0;
                     right: 0;
                   }

                   .c-l-13-imgeEffect > .bottom-left::before {
                     top: 0;
                     left: 0;
                   }

                   .c-l-13-imgeEffect > .bottom-left::after {
                     bottom: 0;
                     left: 0;
                   }

                   .c-l-13-imgeEffect > img {
                     display: block;
                     width: 100%;
                     height: auto;
                     /* filter: blur(4px); */
                     transition: all 0.3s ease-out;
                     z-index: 1;
                   }

                   .c-l-13-imgeEffect:hover::before,
                   .c-l-13-imgeEffect:hover::after,
                   .c-l-13-imgeEffect:hover > .top-right::before,
                   .c-l-13-imgeEffect:hover > .top-right::after,
                   .c-l-13-imgeEffect:hover > .bottom-left::before,
                   .c-l-13-imgeEffect:hover > .bottom-left::after {
                     transform: scale(1.5);
                     opacity: 1;
                   }

                   .c-l-13-imgeEffect:hover::before {
                     background-color: rgba(249, 245, 245, 0.911);
                     transform: translateX(-100%) translateY(-100%);
                   }

                   .c-l-13-imgeEffect:hover::after {
                     background-color: rgba(249, 245, 245, 0.911);
                     transform: translateX(100%) translateY(100%);
                   }

                   .c-l-13-imgeEffect:hover > .top-right::before {
                     background-color: rgba(249, 245, 245, 0.911);
                     transform: translateX(100%) translateY(-100%);
                   }

                   .c-l-13-imgeEffect:hover > .top-right::after {
                     background-color: rgba(249, 245, 245, 0.911);
                     transform: translateX(100%) translateY(100%);
                   }

                   .c-l-13-imgeEffect:hover > .bottom-left::before {
                     background-color: rgba(249, 245, 245, 0.911);
                     transform: translateX(-100%) translateY(-100%);
                   }

                   .c-l-13-imgeEffect:hover > .bottom-left::after {
                     background-color: rgba(254, 253, 253, 0.911);
                     transform: translateX(-100%) translateY(100%);
                   }

                   .c-l-13-imgeEffect:hover > img {
                     /* filter: blur(0); */
                     opacity: 0.9;
                     transform: scale(1.2);

                   }

         /* Media queries for different screen sizes */
         @media only screen and (max-width: 767px) {
